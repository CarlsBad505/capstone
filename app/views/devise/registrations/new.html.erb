<div class="container">
<% if current_user && current_user.admin? %>
  <%= render partial: 'merchant', locals: { resource: resource, resource_name: resource_name } %>
<% else %>
  <%= render partial: 'merchant', locals: { resource: resource, resource_name: resource_name } %>
<% end %>
</div>

